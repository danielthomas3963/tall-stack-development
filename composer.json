{
    "name": "laravel-frontend-presets/tall",
    "description": "TALL preset for Laravel.",
    "keywords": [
        "tall",
        "preset",
        "tailwind",
        "tailwindcss",
        "tailwindui",
        "alpine",
        "alpinejs",
        "laravel",
        "livewire"
    ],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/laravel-frontend-presets/tall/issues",
        "source": "https://github.com/laravel-frontend-presets/tall"
    },
    "authors": [
        {
            "name": "Dan Harrin",
            "email": "danharrin@icloud.com"
        },
        {
            "name": "Liam Hammett",
            "email": "liam@liamhammett.com"
        },
        {
            "name": "Ryan Chandler",
            "email": "41837763+ryangjchandler@users.noreply.github.com"
        }
    ],
    "require": {
        "php": "^7.2.5",
        "laravel/ui": "^2.0",
        "livewire/livewire": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelFrontendPresets\\Tall\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelFrontendPresets\\Tall\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelFrontendPresets\\Tall\\TallServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
